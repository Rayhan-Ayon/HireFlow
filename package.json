{
    "name": "sme-talentflow",
    "version": "1.0.0",
    "description": "SaaS ATS for SMEs",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "server": "node --watch server/index.js",
        "client": "cd client && npm run dev",
        "client-install": "cd client && npm install",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "npm install && npm run client-install && cd client && npm run build",
        "postinstall": "npm run client-install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@azure/msal-node": "^5.0.2",
        "@google/generative-ai": "^0.24.1",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "axios": "^1.13.2",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "googleapis": "^170.1.0",
        "isomorphic-fetch": "^3.0.0",
        "jsonwebtoken": "^9.0.3",
        "mammoth": "^1.11.0",
        "multer": "^2.0.2",
        "nodemailer": "^7.0.12",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}
